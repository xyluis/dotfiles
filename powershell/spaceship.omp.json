{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "yellow",
          "style": "plain",
          "template": "{{ .UserName }} ",
          "type": "session"
        },
        {
          "foreground": "cyan",
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "template": "<#ffffff>in</> {{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "#ff94df",
          "properties": {
            "branch_icon": "<#ff94df><b>\ue0a0 </b></>",
            "fetch_stash_count": true
          },
          "style": "plain",
          "template": "<#ffffff>on</> {{ .HEAD }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }} ",
          "type": "git"
        },
        {
          "type": "node",
          "style": "plain",
          "foreground": "green",
          "template": "<#ffffff>via</> ‚¨¢ v{{ .Full }} "
        },
        {
          "type": "npm",
          "style": "plain",
          "foreground": "red",
          "template": "<#ffffff>via</> üì¶ v{{ .Full }} "
        },
        {
          "type": "executiontime",
          "style": "plain",
          "foreground": "yellow",
          "template": "<#ffffff>took</> {{ .FormattedMs }} ",
          "properties": {
            "threshold": 500,
            "style": "austin"
          }
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "green",
          "style": "plain",
          "template": "‚ùØ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
